{
  "name": "rational-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run config && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "config": "ts-node --esm ./scripts/setenv.mjs",
    "start-dev": "yarn run config && ng serve --host 0.0.0.0 --disable-host-check --ssl=true --ssl-cert=/app/.docker/tls/***REMOVED***.ru.crt --ssl-key=/app/.docker/tls/***REMOVED***.ru.key"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@auth0/angular-jwt": "^5.1.2",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "15",
    "@ngrx/router-store": "15",
    "@ngrx/store": "15",
    "@ngrx/store-devtools": "15",
    "bootstrap": "^5.2.3",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
